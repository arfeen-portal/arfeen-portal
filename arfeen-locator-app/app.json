{
  "expo": {
    "name": "Arfeen Locator",
    "slug": "arfeen-locator",
    "scheme": "arfeenlocator",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "android": {
      "package": "com.arfeen.locator",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ]
    },
    "ios": {
      "bundleIdentifier": "com.arfeen.locator",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "We use your location so your family and Arfeen Travel can track your airport transport."
      }
    }
  }
}
